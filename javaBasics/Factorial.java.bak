class Factor
{
	public static void main(String[] args) 
	{
		int num=234;
		int dup=num;
		int sum=0;
		while (num>0)
		{int rem=num%10;
		int fact=1;
		for (int i=rem;i>=1 ;i-- )
		{
			fact *=i;
        sum+=fact;
		num/=10;
		
				//System.out.println("rem= " + rem+ "fact= "+fact);
		}
			
		}
		
		System.out.println(dup);
		System.out.println("sum of fact of digit"+sum);
		}
	}

